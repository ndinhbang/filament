{
    "name": "filament/filament",
    "description": "Effortlessly build TALL-powered admin panels.",
    "license": "MIT",
    "homepage": "https://github.com/laravel-filament/filament",
    "support": {
        "issues": "https://github.com/laravel-filament/filament/issues",
        "source": "https://github.com/laravel-filament/filament"
    },
    "require": {
        "php": ">=7.4",
        "danharrin/livewire-rate-limiting": "^0.3|^1.0",
        "filament/forms": "self.version",
        "filament/tables": "self.version",
        "illuminate/auth": "^7.29|^8.6|^9.0",
        "illuminate/console": "^7.29|^8.6|^9.0",
        "illuminate/contracts": "^7.29|^8.6|^9.0",
        "illuminate/cookie": "^7.29|^8.6|^9.0",
        "illuminate/database": "^7.29|^8.6|^9.0",
        "illuminate/http": "^7.29|^8.6|^9.0",
        "illuminate/routing": "^7.29|^8.6|^9.0",
        "illuminate/session": "^7.29|^8.6|^9.0",
        "illuminate/support": "^7.29|^8.6|^9.0",
        "illuminate/view": "^7.29|^8.6|^9.0",
        "livewire/livewire": "^2.6",
        "spatie/laravel-package-tools": "^1.9"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Filament\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Filament\\FilamentServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
